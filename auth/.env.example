# Port for the authentication service
PORT=3001

# JWT Configuration
# This should be a long, random, and secret string for signing tokens
JWT_SECRET=your_super_secret_jwt_key

# Cookie Configuration
# The domain for the shared cookie (e.g., .cns-studios.com)
COOKIE_DOMAIN=
# Max age for the cookie in milliseconds (e.g., 7 days = 604800000)
COOKIE_MAX_AGE=604800000
